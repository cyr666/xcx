.team_header{
  box-sizing: border-box;
  padding: 15px 15px 15px 15px;
  background: #fff;
}
.team_keyword{
  display: inline-block;
  box-sizing: border-box;
  padding: 7px 10px;
  margin: 0 10px 10px 0;
  font-size: 28rpx;
  color: #353535;
  border-radius: 4px;
  background: #F3F4F6;
}
.action{
  background:rgba(0,142,255,0.15); 
  color: #008EFF;
}
.team_searchWord{
  box-sizing: border-box;
  padding:10px 0 10px 15px;
  font-size: 22rpx;
  color: #888888
}
.people_num{
  color: red;
}
.list_con{
  margin: 0 15px;
}
.list_item{
  box-sizing: border-box;
  padding: 20px 20px 15px 20px;
  background: #fff;
  margin-bottom: 15px;
  border-radius: 5px;
}
.list_header{
  overflow: hidden;
}
.logo_con{
  float: left;
  width: 60%;
  font-size: 34rpx;
  color: #000;
  font-weight: bold;
}
.list_symbol{
  display: inline-block;
  margin-right: 1px;
  width: 24px;
  height: 24px;
  font-size: 32rpx;  
  line-height: 24px;
  text-align: center;
  background: pink; 
  color: #fff;
}
.list_fen{
  float: right;
  margin-top: 3px;
  font-size: 24rpx;
  color: #757575;
}
.chan_con{
  font-size: 28rpx;
  color: #888;
  margin-top: 10px;
  overflow: hidden;
}
.chan_title{
  float: left;
  width: 199rpx;
}
.chanItem_con{
  float: left;
  width: 390rpx; 
}
.chan_item{
  display: inline-block;
  box-sizing: border-box;
  padding: 3px 5px;
  margin: 0 5px 5px 0;
  border: 1px solid #008EFF;
  border-radius: 2px;
  color: #008EFF;
}
.footer{
  font-size:22rpx;
  color:rgba(136,136,136,1);
  text-align: center;
}