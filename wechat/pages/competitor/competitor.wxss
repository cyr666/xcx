.list_con{
  box-sizing: border-box;
  background: #fff;
  padding: 0 15px;
}
.detail_list_item{
  position: relative;
  box-sizing: border-box;
  padding: 20px 0 15px 0;
  background: #fff;
  border-bottom: 1px solid #E5E5E5;
  
  /* margin-top: 15px; */
  /* border-radius: 5px; */
}
.list_header{
  overflow: hidden;
}
.logo_con{
  float: left;
  width: 85%;
  font-size: 34rpx;
}
.list_symbol{
  display: inline-block;
  margin-right: 5px;
  width: 24px;
  height: 24px;
  font-size: 32rpx; 
  line-height: 24px;
  text-align: center;
  background: pink; 
  color: #fff;
}
.list_fen{
  float: right;
  font-size: 24rpx;
  color: #757575;
}
.fen{
  font-size: 36rpx;
  color: #F96060;
}
.detail_chan_con{
  font-size: 28rpx;
  color: #888;
  margin-top: 10px;
  overflow: hidden;
}
.detail_chan_title{
  font-size: 24rpx;
  float: left;
  width: 20%;
}
.detail_chanItem_con{
  float: left;
  width: 80%; 
}
.detail_chan_item{
  display: inline-block;
  box-sizing: border-box;
  padding: 3px 5px;
  margin: 0 5px 5px 0;
  border: 1px solid #008EFF;
  border-radius: 2px;
  color: #008EFF;
}
.chan_right{
  float: right;
  overflow: hidden;
  height: 45px;
  display:-webkit-flex; 
  justify-content:center;
  align-items:center;

}
.chan_people{
  display: inline-block;
  margin-right: 10px;
  font-size: 26rpx;
  color: #008EFF;
  line-height: 45px;
}
.right_jiantou{
  width: 10px;
  height: 20px;
  margin-right: 15px;
}
.chickDetail_con{
  position: absolute;
  top: 48%;
  right: 15px;
  width: 10px;
  height: 20px;
}
.right_gray_img{
  width: 10px;
  height: 20px;
}
.comImg{
  width: 24px;
  height: 24px;
  vertical-align: middle;
  margin-top: -2px;
}