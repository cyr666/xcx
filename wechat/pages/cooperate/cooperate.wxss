.cooperateTable_con,.cooperateTable_tr{
  width: 100%;
  overflow: hidden;
}
.cooperateTable_head{
  width: 100%;
  height: 58rpx;
  border-top: 1px solid #DCDCDC;
  border-bottom: 1px solid #DCDCDC;
  background: #F9F9F9;
}
.cooperateTableHead_left,.cooperateTableHead_right{
  box-sizing: border-box;
  padding-left: 30rpx;
  float: left;
  width: 305rpx;
  font-size: 24rpx;
  line-height: 58rpx;
  color: #7B8185;
}
.cooperateTableHead_right{
  float: left;
  width: 444rpx;
  border-left: 1px solid #DCDCDC;
}
.cooperateTable_tr{
  display: flex;
  align-items: center;
  border-bottom: 1px solid #DCDCDC;
}
.cooperateTableTr_left,.cooperateTableTr_right{
  box-sizing: border-box;
  padding-left: 30rpx;
  float: left;
  width: 305rpx;
  background: #F9F9F9;
  font-size: 24rpx;
  color:#353535;
}
.cooperateTableTr_right{
  padding-left: 0;
  width: 444rpx;
  border-left: 1px solid #DCDCDC;
  background: #fff;
}
.cooperateTableTr_td{
  box-sizing: border-box;
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #E5E5E5;
}
.borderNone{
  border-bottom: none;
}
.footer{
  font-size:22rpx;
  color:rgba(136,136,136,1);
  line-height:30px; 
  text-align: center;
}